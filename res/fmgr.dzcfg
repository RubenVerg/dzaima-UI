        }
      }
      hsep{}
      pad { x=.2em y=.2em
        h { alX=center h=min
          scroll { y=off x=hidden
            codefield { family="Arial" text="" id=path }
          }
        }
      }
      pad { x=.2em y=.4em
        h { alX=center w=min
          btn { id=bookmark pad=.3em svgicon { w=24 h=24 sz=10 color=icon.color src="M17,3H7A2,2 0 0,0 5,5V21L12,18L19,21V5C19,3.89 18.1,3 17,3Z" } } // M17,3A2,2 0 0,1 19,5V21L12,18L5,21V5C5,3.89 5.9,3 7,3H17M11,14L17.25,7.76L15.84,6.34L11,11.18L8.41,8.59L7,10L11,14Z
        }
      }
      hsep{}
      pad { x=.2em y=.4em
        h { pad=.1em alX=center w=min
          btn { id=refresh pad=.3em svgicon { w=24 h=24 sz=10 color=icon.color src="M17.65,6.35C16.2,4.9 14.21,4 12,4A8,8 0 0,0 4,12A8,8 0 0,0 12,20C15.73,20 18.84,17.45 19.73,14H17.65C16.83,16.33 14.61,18 12,18A6,6 0 0,1 6,12A6,6 0 0,1 12,6C13.66,6 15.14,6.69 16.22,7.78L13,11H20V4L17.65,6.35Z" } }
