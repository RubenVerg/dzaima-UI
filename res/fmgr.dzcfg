fmgr {
  ui = v {
    h { bg=bg.middle alY=center h=min
      pad { x=.2em y=.4em
        h { pad=.1em alX=center w=min
          // https://github.com/Templarian/MaterialDesign/blob/master/LICENSE https://www.apache.org/licenses/LICENSE-2.0
          btn { id=prev pad=.3em svgicon { w=24 h=24 sz=10 color=icon.color src="M20,11V13H8L13.5,18.5L12.08,19.92L4.16,12L12.08,4.08L13.5,5.5L8,11H20Z" } }
          btn { id=next pad=.3em svgicon { w=24 h=24 sz=10 color=icon.color src="M4,11V13H16L10.5,18.5L11.92,19.92L19.84,12L11.92,4.08L10.5,5.5L16,11H4Z" } }
